{
  "manifest_version": 3,
  "name": "Network Request Monitor",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
